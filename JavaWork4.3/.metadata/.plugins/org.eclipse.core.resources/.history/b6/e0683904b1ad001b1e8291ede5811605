package Concrete;

import Abstruct.GameService;
import Entity.Game;

public class GameManager implements GameService {

	@Override
	public void Add(Game game) {
		System.out.println(game.getId() +" "+game.getGameName()+" "+game.getPublishDate()+" "+game.getGameCategory().getCategoryName()+" Oyun Eklendi.");
		
	}

	@Override
	public void Update(Game game) {
		System.out.println(game.getId() +" 'li Oyun Güncellendi.");
		
	}

	@Override
	public void Delete(Game game) {
		System.out.println(game.getId() +" 'li Oyun Silindi.");
		
	}

}
